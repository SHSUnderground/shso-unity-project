<vo-action-data>
  <vo-actions>

    <!-- VO Action example -->
    <vo-action>
      <name>example</name> <!-- Can be anything; this is the string that VOObjects or code use to reference this action -->

      <!-- Inputs are how "{n}" strings in "clip" elements are filled in (see below) -->
      <inputs>
        <input>
          <class>VOInputString</class> <!-- A code class can be specified, provided that class implements the IVOInputResolver interface -->
          <params>
            <param>Vibranium</param> <!-- Class-specific parameters can be given to the input instance; In this case, the first parameter is used directly -->
          </params>
        </input>
        <input /> <!-- Empty inputs like this one are used to specify "run-time inputs."  This second input, for example, must be provided by a VOObject or given in the code -->
      </inputs>

      <!-- Clips are where we specify assets.  A clip is chosen randomly during resolution. -->
      <!-- Note: If a resolved bundle/asset pair is not specified in vo_assets.xml, then it won't be played! -->
      <clips>
        <clip>
          <bundle>hq_physics</bundle> <!-- The base audio bundle name that contains the asset (can use {n} strings) -->
          <asset>Phy_Materials_{0}_Large_1</asset> <!-- The actual asset name*; in this case it would get resolved to 'Phy_Materials_Vibranium_Large_1' -->
          <text-id>#{1}_SOME_TEXT_ID</text-id> <!-- For captions, a text id is specified.  This example uses a run-time input. -->
        </clip>
      </clips>

      <!-- * If the actual bundled asset is an audio prefab, still specify the root name (sans "_audio").  VO resolution will check for the existence of prefabs first. -->

      <!-- Routing information defines how an action is handled relative to other actions already playing. -->
      <routing-info>
        <priority>2</priority> <!-- How important this action is relative to other actions, where more important VO will have a larger priority. -->
        <important>true</important> <!-- If true, all other playing VO will be canceled, and no additional VO will be played while the important VO is playing. -->
        <cuts-self>true</cuts-self> <!-- If true, additional instances of this action played from the same emitter will cancel the current instance, otherwise the new instance will be ignored. -->
        <queues-self>true</queues-self> <!-- If true, multiple instances of this action played from different emitters will be queued in a first-in, first-out fashion. -->
      </routing-info>

      <!-- Extra options are specified here -->
      <disabled>true</disabled> <!-- For active development (or for hotfixes), actions can be disabled -->
      <local>true</local> <!-- VO actions are networked by default, but can be made local-only using this element -->
      <play-on-unowned>true</play-on-unowned> <!-- VO actions are never played on unowned emitters, unless this flag is set to true -->
      <play-over-ui>true</play-over-ui> <!-- VO is muted by default when full-screen UI is opened; this option allows this action to play in that case. -->

    </vo-action>
    <!-- End VO Action example -->

